{
	"name": "theme-japandi",
	"version": "0.1.2",
	"displayName": "Japandi Theme",
	"homepage": "https://japandi.dev/",
	"description": "Japandi combines the minimalism of Japanese design with the elegance of Scandinavian style to create a coding environment that's serene and focused.",
	"publisher": "Japandi",
	"license": "MIT",
	"scripts": {
		"package": "vsce package -o ./bin/japandi.vsix",
		"vsce-publish": "vsce publish"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/webhooked/japandi-theme.git"
	},
	"bugs": {
		"url": "https://github.com/webhooked/japandi-theme/issues"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": ["Themes"],
	"keywords": [
		"japandi",
		"minimalistic",
		"minimalism",
		"dark",
		"theme",
		"color-theme"
	],
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#131418",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "Japandi Midnight",
				"uiTheme": "vs-dark",
				"path": "./themes/japandi-midnight.json"
			},
			{
				"label": "Japandi Nightfall",
				"uiTheme": "vs-dark",
				"path": "./themes/japandi-nightfall.json"
			}
		]
	},
	"devDependencies": {
		"prettier": "^3.0.0",
		"vsce": "^2.15.0"
	}
}
